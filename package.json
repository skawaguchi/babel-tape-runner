{
  "name": "babel-tape-no-css",
  "version": "1.0.0",
  "description": "Babel + Tape for running your ES Next tests but ignores CSS.",
  "bin": "./bin/babel-tape-runner",
  "repository": {
    "type": "git",
    "url": "https://github.com/skawaguchi/babel-tape-runner.git"
  },
  "scripts": {
    "pretest": "cp .testbabelrc .babelrc",
    "test": "standard bin/babel-tape-runner -f test.js && node bin/babel-tape-runner -f test.js",
    "posttest": "rm .babelrc"
  },
  "keywords": [
    "babel",
    "tape",
    "tests",
    "testing",
    "harmony",
    "es6",
    "esnext"
  ],
  "author": "Marc Harter <wavded@gmail.com>",
  "license": "MIT",
  "homepage": "https://github.com/skawaguchi/babel-tape-runner/tree/temp-release",
  "dependencies": {
    "babel-polyfill": "^6.3.14",
    "babel-register": "^6.3.13",
    "glob": "^6.0.1",
    "ignore-styles": "^3.0.0"
  },
  "devDependencies": {
    "babel-preset-es2015": "^6.3.13",
    "standard": "^5.0.0",
    "tape": "^4.0.2"
  }
}
